feature frac {

	@NUMERATOR = [zero.numerator one.numerator two.numerator three.numerator four.numerator five.numerator six.numerator seven.numerator eight.numerator nine.numerator period.numerator comma.numerator hyphen.numerator parenleft.numerator parenright.numerator dollar.numerator cent.numerator];
	@DENOMINATOR = [zero.denominator one.denominator two.denominator three.denominator four.denominator five.denominator six.denominator seven.denominator eight.denominator nine.denominator period.denominator comma.denominator hyphen.denominator parenleft.denominator parenright.denominator dollar.denominator cent.denominator];
	@PREBUILT = [perthousand percent onequarter onehalf threequarters onethird twothirds oneeighth threeeighths fiveeighths seveneighths];
	
	lookup FRAC {
		sub [zero zero.fitted zero.oldstyle zero.taboldstyle zero.superior zero.inferior zero.denominator zero.slash zero.slashfitted] by zero.numerator;
		sub [one one.fitted one.oldstyle one.taboldstyle one.superior one.inferior one.denominator] by one.numerator;
		sub [two two.fitted two.oldstyle two.taboldstyle two.superior two.inferior two.denominator] by two.numerator;
		sub [three three.fitted three.oldstyle three.taboldstyle three.superior three.inferior three.denominator] by three.numerator;
		sub [four four.fitted four.oldstyle four.taboldstyle four.superior four.inferior four.denominator] by four.numerator;
		sub [five five.fitted five.oldstyle five.taboldstyle five.superior five.inferior five.denominator] by five.numerator;
		sub [six six.fitted six.oldstyle six.taboldstyle six.superior six.inferior six.denominator] by six.numerator;
		sub [seven seven.fitted seven.oldstyle seven.taboldstyle seven.superior seven.inferior seven.denominator] by seven.numerator;
		sub [eight eight.fitted eight.oldstyle eight.taboldstyle eight.superior eight.inferior eight.denominator] by eight.numerator;
		sub [nine nine.fitted nine.oldstyle nine.taboldstyle nine.superior nine.inferior nine.denominator] by nine.numerator;
		sub [period period.denominator period.superior period.inferior] by period.numerator;
		sub [comma comma.denominator comma.superior comma.inferior] by comma.numerator;
		sub [hyphen hyphen.denominator hyphen.superior hyphen.inferior sfthyphen] by hyphen.numerator;
		sub [parenleft parenleft.denominator parenleft.superior parenleft.inferior parenleft.cap] by parenleft.numerator;
		sub [parenright parenright.denominator parenright.superior parenright.inferior parenright.cap] by parenright.numerator;
		sub [dollar dollar.oldstyle dollar.taboldstyle dollar.denominator dollar.superior dollar.inferior] by dollar.numerator;
		sub [cent cent.oldstyle cent.taboldstyle cent.denominator cent.superior cent.inferior] by cent.numerator;
		sub [slash divisionslash] by fraction;
	} FRAC;
	
	sub zero.numerator' fraction' zero.numerator' zero.numerator' by perthousand;
	sub zero.numerator' fraction' zero.numerator' by percent;
	sub one.numerator' fraction' four.numerator' by onequarter;
	sub one.numerator' fraction' two.numerator' by onehalf;
	sub three.numerator' fraction' four.numerator' by threequarters;
	sub one.numerator' fraction' three.numerator' by onethird;
	sub two.numerator' fraction' three.numerator' by twothirds;
	sub one.numerator' fraction' eight.numerator' by oneeighth;
	sub three.numerator' fraction' eight.numerator' by threeeighths;
	sub five.numerator' fraction' eight.numerator' by fiveeighths;
	sub seven.numerator' fraction' eight.numerator' by seveneighths;
	
	sub [fraction @DENOMINATOR @PREBUILT] @NUMERATOR' by @DENOMINATOR;

} frac;


